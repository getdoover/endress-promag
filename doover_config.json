                "eh_meter_port": {
                    "title": "EH Meter Port",
                    "x-name": "eh_meter_port",
                    "x-hidden": false,
                    "type": "integer",
                    "default": 80
                },
                "eh_meter_password": {
                    "title": "EH Meter Password",
                    "x-name": "eh_meter_password",
                    "x-hidden": false,
                    "type": "string",
                    "default": "0000"
